<p id="notice"><%= notice %></p>

<h1>Location Lists</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_lists.each do |location_list| %>
      <tr>
        <td><%= location_list.profile %></td>
        <td><%= link_to 'Show', location_list %></td>
        <td><%= link_to 'Edit', edit_location_list_path(location_list) %></td>
        <td><%= link_to 'Destroy', location_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location List', new_location_list_path %>
